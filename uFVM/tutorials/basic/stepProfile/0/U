FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (2 1 0);

boundaryField
{
    inlet1
    {
        type            fixedValue;
        value           uniform (2 1 0);
    }

    inlet2
    {
        type            fixedValue;
        value           uniform (2 1 0);
    }
    outlet
    {
         type            fixedValue;
        value           uniform (2 1 0);
   }

    frontAndBack
    {
        type            empty;
    }
}

