FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        relTol          0;
    }

    U
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-05;
        relTol          0;
    }
}

SIMPLE
{
    nCorrectors     2;
    nNonOrthogonalCorrectors 2;
    residualControl
    {
        p               0.000001;
        U               0.000001;
    }
}

relaxationFactors 
{ 
    fields 
    { 
        p               0.3; 
    } 
    equations 
    { 
        U               0.7;
    } 
} 


