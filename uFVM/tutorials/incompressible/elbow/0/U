FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    wall-1
    {
        type            noSlip;
    }

    inlet-1
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }

    inlet-2
    {
        type            fixedValue;
        value           uniform (0 3 0);
    }

    outlet
    {
        type            zeroGradient;
    }

    wall-2
    {
        type            noSlip;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}

